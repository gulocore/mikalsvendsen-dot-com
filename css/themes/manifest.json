{
  "defaultTheme": "nordic-twilight",
  "themes": [
    {
      "id": "nordic-twilight",
      "name": "Nordic Twilight",
      "description": "A dark theme inspired by the Northern Lights",
      "author": "Mikal Svendsen",
      "version": "1.0.0",
      "isDefault": true,
      "hasJavaScript": false,
      "jsPath": null,
      "dependencies": [],
      "type": "dark",
      "cssPath": "/css/default.css"
    },
    {
      "id": "nordic-twilight-3D",
      "name": "Nordic Twilight 3D",
      "description": "Enhanced Nordic Twilight theme with 3D background effects and particle systems",
      "author": "Theme Developer",
      "version": "1.0.0",
      "isDefault": false,
      "hasJavaScript": true,
      "jsPath": "/js/themes/nordic-twilight-3D.js",
      "dependencies": [],
      "type": "dark",
      "cssPath": "/css/themes/nordic-twilight-3D.css"
    },
    {
      "id": "skybox-3D",
      "name": "Skybox 3D",
      "description": "Immersive 3D HDR skybox background with glass morphism UI and scroll parallax",
      "author": "Mikal Svendsen",
      "version": "1.0.0",
      "isDefault": false,
      "hasJavaScript": true,
      "jsPath": "/js/themes/skybox-3D.js",
      "dependencies": [],
      "type": "light",
      "features": [
        "3d-background",
        "hdr-skybox",
        "glass-morphism",
        "scroll-parallax",
        "performance-detection",
        "mobile-optimized"
      ],
      "requirements": {
        "webgl": true,
        "threejs": "r128+",
        "minMemory": "2GB"
      },
      "compatibility": {
        "mobile": true,
        "tablet": true,
        "desktop": true,
        "lowEnd": "fallback"
      },
      "cssPath": "/css/themes/skybox-3D.css"
    }
  ]
}